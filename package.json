{
  "name": "remitano-server",
  "version": "0.0.1",
  "description": "remitano-server",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": "14 || 16 || 17 || 18"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "yarn run eslint && yarn run prettier:check",
    "lint:fix": "yarn run eslint:fix && yarn run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "yarn run prettier:cli -l",
    "prettier:fix": "yarn run prettier:cli --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "yarn run eslint --fix",
    "pretest": "yarn run rebuild",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "yarn run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && yarn run posttest",
    "docker:build": "docker-compose build",
    "docker:run": "docker-compose up -d",
    "docker:deploy": "docker-compose build && docker-compose up -d",
    "premigrate": "yarn run build",
    "migrate": "node ./dist/migrate",
    "preopenapi-spec": "yarn run build",
    "openapi-spec": "node ./dist/openapi-spec",
    "prestart": "yarn run rebuild",
    "start": "node -r source-map-support/register .",
    "start:watch": "tsc-watch --target es2017 --outDir ./dist --onSuccess \"env-cmd -f ./.env.dev node .\"",
    "start:watch:local": "tsc-watch --target es2017 --outDir ./dist --onSuccess \"env-cmd -f ./.env.local node .\"",
    "dev": "npm run pretest && npm run start:watch",
    "dev:local": "npm run pretest && npm run start:watch:local",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
    "rebuild": "yarn run clean && yarn run build"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Tuan Pham <tuanpha.it@gmail.com>",
  "license": "ISC",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/authentication": "^10.0.0",
    "@loopback/authentication-jwt": "^0.13.0",
    "@loopback/authorization": "^0.13.0",
    "@loopback/boot": "^6.0.0",
    "@loopback/core": "^5.0.0",
    "@loopback/repository": "^6.0.0",
    "@loopback/rest": "^13.0.0",
    "@loopback/rest-explorer": "^6.0.0",
    "@loopback/service-proxy": "^6.0.0",
    "@loopback/socketio": "^0.7.0",
    "aws-sdk": "^2.1047.0",
    "colorthief": "^2.3.2",
    "express-serve-static-core": "^0.1.1",
    "fluent-ffmpeg": "^2.1.2",
    "isemail": "^3.2.0",
    "loopback-connector-postgresql": "^5.0.1",
    "moment": "^2.29.1",
    "multer": "^1.4.2",
    "nodemailer": "^6.6.0",
    "perf_hooks": "^0.0.1",
    "prettier": "^2.8.8",
    "ramda": "^0.27.1",
    "sharp": "^0.29.3",
    "socket.io": "^4.6.1",
    "tslib": "^2.0.0",
    "uuid": "^8.3.2",
    "uuid-random": "^1.3.2"
  },
  "devDependencies": {
    "@loopback/build": "^10.0.0",
    "@loopback/eslint-config": "^14.0.0",
    "@loopback/testlab": "^6.0.0",
    "@types/fluent-ffmpeg": "^2.1.20",
    "@types/multer": "^1.4.5",
    "@types/node": "^14.18.47",
    "@types/nodemailer": "^6.4.1",
    "@types/ramda": "^0.27.62",
    "@types/sharp": "^0.29.5",
    "@types/uuid": "^8.3.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8.40.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "source-map-support": "^0.5.21",
    "tsc-watch": "^4.5.0",
    "typescript": "~4.9.5"
  }
}
